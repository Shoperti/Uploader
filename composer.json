{
    "name": "shoperti/uploader",
    "description": "Smart uploader for files for Laravel",
    "keywords": [
        "laravel",
        "uploader"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/support": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/filesystem": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "illuminate/http": "5.0.*|5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "intervention/image": "^2.4"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3|^4.0",
        "phpunit/phpunit": "^4.8|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Shoperti\\Uploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shoperti\\Tests\\Uploader\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Shoperti\\Uploader\\UploaderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
