{
    "name": "shoperti/uploader",
    "description": "Smart file uploader for Laravel",
    "keywords": [
        "laravel",
        "uploader"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/contracts": "5.7.* || 5.8.*",
        "illuminate/support": "5.7.* || 5.8.*",
        "illuminate/filesystem": "5.7.* || 5.8.*",
        "illuminate/http": "5.7.* || 5.8.*",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.5",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Shoperti\\Uploader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shoperti\\Tests\\Uploader\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.8-dev"
        },
        "laravel": {
            "providers": [
                "Shoperti\\Uploader\\UploaderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
